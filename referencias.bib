doi = {10.3390/e21100932},
file = {:C$\backslash$:/Users/francisco.plaza.vega/Documents/Doctorado/Tesis/Articulos/Gonzalez-Venegas{\_}etal{\_}2019 - AKL-ABC automatic ABC based on kernel learning.pdf:pdf},
journal = {Entropy},
